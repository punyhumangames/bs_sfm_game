"VertexlitGeneric"
{
	"$basetexture" "models\props_community\com_retscanner_eye"
	"$model" 1
    "$angle" 0.0
    "$translate" "[0.0 0.0]"
    "$center" "[2.5 .5]"

	
	"$bumpmap"	"models/shared/n_flat"
	"$phongexponenttexture" "models/shared/albedotint"
	"$surfaceprop" "metal"
	"$envmaptint" "[.40 .20 .20]"
	"$envmapcontrast" .5
	"$normalmapalphaenvmapmask" "1"
	"$lightwarptexture" "models/shared/lightwarp_darken"
	"$rimlight" "1"
	"$rimlightboost" "100"
	$rimlightexponent  1
	"$envmap" "env_cubemap"
	"$phong" 1
	"$phongfresnelranges"	"[0 .5 1]"
	"$phongexponent" 50
	"$phongboost" "5"
	"$selfillum" 1
	"$selfillumtint" "[10 0 0]"
    "Proxies"
    {
        "LinearRamp"
        {
            "rate" -45
            "initialValue" 0.0
            "resultVar" "$angle"
        }

        "TextureTransform"
        {
            "translateVar" "$translate" // we don't need this, but it's not optional
            "rotateVar" "$angle"
            "centerVar" "$center"
            "resultVar" "$basetexturetransform"
        }
    } 
}
